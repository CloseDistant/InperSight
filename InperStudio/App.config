<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="System.Configuration.IgnoreSectionHandler" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <log4net>
    <!--定义输出到文件中-->
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <!--定义文件存放位置-->
      <file value="Log//" />
      <!--如果放在Debug下,当然名字你可以改 -->
      <!--<file value="log//logfile.txt"/>-->
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="'_'yyyy-MM-dd'.txt'" />
      <param name="StaticLogFileName" value="false" />
      <!--<param name="MaxSizeRollBackups" value="1" />-->
      <layout type="log4net.Layout.PatternLayout">
        <!--输出格式-->
        <!--样例：2008-03-26 13:42:32,111 [10] INFO  Log4NetDemo.MainClass [(null)] - info-->
        <conversionPattern value="记录时间：%date 线程ID:[%thread] 日志级别：%-5level 所在类：%logger property:[%property{NDC}] - 日志描述：%message%newline" />
      </layout>
    </appender>
    <!--定义日志的输出媒介-->
    <root>
      <!--文件形式记录日志-->
      <appender-ref ref="LogFileAppender" />
    </root>
  </log4net>
  <appSettings>
    <!--<add key="version" value="V1.1.2" />-->
    <add key="version" value="V3.6.4" />
    <add key="releaseData" value="2022-05-01" />
    <add key="versionDesc" value="1.缺陷修复" />
    <add key="mcu_version" value="V1.0.0" />
    <!--正式服务-->
    <!--<add key="conStr" value="nkHBpgCGzvvHRoMgCCrvn0jVpPPXu8TeAHkERGLrxKB3LQo895NV8EtYNxR5xeGgR9hkxHrK6eq07BydJ7smcWATGx4BPs0wjFIFS0o8RrBcrqJ1gudG4uoLw85DoZwSSE114bMhL5oRyEoykiTJEHjJA2GmyN/THPetUr3vDpDsUlDcwEvturkdeu6Dyuks4C5s7YtZ78RX1ezbHR18b9rDZ8GeQ12Zj6X/s6qWpkw=" />
    <add key="upgradeKey" value="UpgradeFileEnv/" />-->
    <!--开发服务-->
    <add key="conStr" value="nkHBpgCGzvvHRoMgCCrvn0jVpPPXu8TeAHkERGLrxKB3LQo895NV8EtYNxR5xeGgR9hkxHrK6eq07BydJ7smcWATGx4BPs0wjFIFS0o8RrBcrqJ1gudG4uoLw85DoZwSSE114bMhL5qArJQX3pQ2ymgG9z/5PwHHR05FEkrOfKMOnTnCxMEDors44fonJEHtz6VgMzy9LvMwEHAkdZAuYAGN2qUO2w28kuGfPfcHoxw=" />
    <add key="upgradeKey" value="UpgradeFile/" />
    <!--<add key="mcuKey" value="Mcu/" />-->
     <add key="mcuKey" value="McuEnv/" />
    <add key="isSkip" value="false" />
    <add key="dataPath" value="123212" />
    <add key="dataCustomDirectoryPath" value="" />
    <add key="configPath" value="" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <add key="themeColor" value="#6523A5" />
    <add key="language" value="en_us" />
  </appSettings>
  <runtime>
